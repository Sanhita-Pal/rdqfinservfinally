
@{
    ViewBag.Title = "Rdq Finserv Providing EMI Protection for EMI Payment";
    ViewBag.description = "Rdq Finserv no 1 Fintech Company in Kolkata & India, Providing EMI Protection for EMI Payment. For more inquiry Call Us: 033-68260999";
    ViewBag.keyword = "EMI Payment, EMI Protection, online EMI payment, EMI finance, no 1 fintech company in kolkata, no 1 fintech company in east india, best fintech company in india";
    ViewBag.url = "https://rdqfinserv.com/sign-up";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<style>

    /* loading dots */

    .loading:after {
        content: ' .';
        animation: dots 1s steps(5, end) infinite;
    }

    @@keyframes dots {
        0%, 20% {
            color: rgba(0,0,0,0);
            text-shadow: .25em 0 0 rgba(0,0,0,0), .5em 0 0 rgba(0,0,0,0);
        }

        40% {
            color: red;
            text-shadow: .25em 0 0 rgba(0,0,0,0), .5em 0 0 rgba(0,0,0,0);
        }

        60% {
            text-shadow: .25em 0 0 red, .5em 0 0 rgba(0,0,0,0);
        }

        80%, 100% {
            text-shadow: .25em 0 0 red, .5em 0 0 red;
        }
    }
</style>


<section>
    <div class="container">
        <div class="col-md-12">

            <div class="section-heading wow fadeIn mb-1 text-center" data-wow-duration="0.3s">
                <h4 style="padding-top:20px; margin-bottom:0px;"><span style="color:blue">Coming </span><em class="loading"> Soon </em></h4>
                <span class="section-title-border text-center" style="margin-left:47%;margin-top:2%"></span>
                <h1 class="text-center" style="line-height: 1.3;">
                    EMI Protection
                </h1>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3"></div>
            <div class="col-md-6 text-center">
                <img src="~/Assets/images/emi-protection/emi-protection.png" class="mycard w-75 " style="box-shadow:none" alt="emi-protection">
            </div>
            <div class="col-md-3"></div>
        </div>
    </div>
</section>

